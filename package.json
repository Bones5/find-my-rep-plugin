{
  "name": "find-my-rep-plugin",
  "version": "1.0.0",
  "description": "A WordPress plugin for contacting local representatives",
  "main": "build/index.js",
  "scripts": {
    "build": "wp-scripts build src/index.tsx src/frontend.tsx --output-path=build && cp src/style.css build/style.css",
    "start": "wp-scripts start src/index.tsx src/frontend.tsx --output-path=build",
    "lint:js": "wp-scripts lint-js src",
    "format:js": "wp-scripts format-js src",
    "env:start": "wp-env start",
    "env:stop": "wp-env stop",
    "env:clean": "wp-env clean all",
    "env:logs": "wp-env logs",
    "test:phpunit": "vendor/bin/phpunit",
    "test:jest": "jest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "keywords": [
    "wordpress",
    "gutenberg",
    "block",
    "representatives"
  ],
  "author": "Bones5",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/wordpress__block-editor": "^14.21.1",
    "@types/wordpress__blocks": "^12.5.18",
    "@types/wordpress__components": "^23.0.12",
    "@types/wordpress__element": "^2.4.1",
    "@types/wordpress__i18n": "^3.4.0",
    "@wordpress/env": "^10.33.0",
    "@wordpress/scripts": "^26.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@wordpress/block-editor": "^12.0.0",
    "@wordpress/blocks": "^12.0.0",
    "@wordpress/components": "^25.0.0",
    "@wordpress/element": "^5.0.0",
    "@wordpress/i18n": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
